<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.ColumnConstraints?>

<?import javafx.scene.layout.RowConstraints?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Label?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="org.usfirst.frc.team2832.widgets.widget.LogWidget"
          fx:id="root"
          minWidth="64" minHeight="64"
          prefHeight="128" prefWidth="128">
    <Label text="Pull Logs" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
    <Button fx:id="button" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
    <ComboBox fx:id="comboBox" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    <padding>
        <Insets topRightBottomLeft="8"/>
    </padding>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" percentHeight="50"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" percentHeight="50"/>
    </rowConstraints>
    <columnConstraints>
        <ColumnConstraints minWidth="64.0" percentWidth="30.0" prefWidth="100.0"/>
        <ColumnConstraints minWidth="64.0" percentWidth="70.0" prefWidth="100.0"/>
    </columnConstraints>
</GridPane>
